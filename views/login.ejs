<!DOCTYPE html>
<html>

<head>
  <% include ./head %>
    <link rel='stylesheet' href='/stylesheets/login.css'>
</head>

<body>
  <div class="container">
    <% include ./navNoDropdown %>
      <form id="login-form">
        <div class="form-group">
          <label for="InputEmail" class="col-form-label">Email address</label>
          <input type="email" class="form-control" id="InputLoginEmail" aria-describedby="emailHelp" placeholder="Enter email">
          <p id="emailStatus"></p>
        </div>
        <div class="form-group">
          <label for="InputPassword" class="col-form-label">Password</label>
          <input type="password" class="form-control" id="InputLoginPassword" placeholder="Password">
          <p id="passwordStatus"></p>
        </div>

        <button type="submit" class="btn btn-primary">Login</button>
      </form>
  </div>
  <!--end of container-->
  <script src="/javascripts/feLogin.js"></script>
</body>

</html>